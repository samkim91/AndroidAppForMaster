<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingsButton">
        <attr name="text" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="TitleTextView">
        <attr name="title_text" format="reference|string" />
        <attr name="detail_text" format="reference|string" />
        <attr name="detail_bg" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="TitleTextView2">
        <attr name="title_text2" format="reference|string" />
        <attr name="detail_text2" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="TitleEditTextView">
        <attr name="title_edit_title_text" format="reference|string" />
        <attr name="title_edit_detail_text" format="reference|string" />
        <attr name="title_edit_hint_text" format="reference|string" />
        <attr name="title_edit_hint_visible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TitleSwitchCompat">
        <attr name="title_switch_title_text" format="reference|string" />
        <attr name="title_switch_state" format="boolean" />
    </declare-styleable>

</resources>