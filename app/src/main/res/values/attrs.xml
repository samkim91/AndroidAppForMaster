<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingsButton">
        <attr name="text" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="TitleTextView">
        <attr name="title_text" format="reference|string" />
        <attr name="detail_text" format="reference|string" />
        <attr name="detail_bg" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="NotoTextView">
        <attr name="fontweight" format="enum">
            <enum name="regular" value="0" />
            <enum name="medium" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>

</resources>